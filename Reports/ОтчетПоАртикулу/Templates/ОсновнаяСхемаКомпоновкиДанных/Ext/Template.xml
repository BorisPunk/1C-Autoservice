		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЗаказНаряд.Ссылка,
	ЗаказНаряд.Заказчик,
	ЗаказНаряд.Материалы.(
		Артикул,
		Количество,
		Цена,
		Сумма
	),
	ЗаказНаряд.Дата,
	ЗаказНаряд.Автомобиль.ГосНомер
ИЗ
	Документ.ЗаказНаряд КАК ЗаказНаряд
ГДЕ
	ЗаказНаряд.Материалы.Артикул = &amp;Артикул
	И ЗаказНаряд.Заказчик = &amp;Заказчик
	И ЗаказНаряд.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
{ГДЕ
	ЗаказНаряд.Материалы.(
		Артикул.*
	) ,
	ЗаказНаряд.Дата,
	ЗаказНаряд.Заказчик.*}</query>
